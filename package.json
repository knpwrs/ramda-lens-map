{
  "name": "ramda-lens-map",
  "version": "1.0.1",
  "description": "A lens for ramda which lets you focus on a key in a native Map.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "prepack": "npm run build",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "test": "jest --coverage --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knpwrs/ramda-lens-native.git"
  },
  "keywords": [
    "ramda",
    "lens",
    "map"
  ],
  "author": "Kenneth Powers <ken@kenpowers.net> (https://knpw.rs)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knpwrs/ramda-lens-native/issues"
  },
  "homepage": "https://github.com/knpwrs/ramda-lens-native#readme",
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.3.0",
    "@types/jest": "^23.3.10",
    "@types/ramda": "^0.25.42",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-typescript": "^0.11.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "^2.8.1",
    "typescript-eslint-parser": "^21.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
